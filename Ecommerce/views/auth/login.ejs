  </head>
  <body>
    <%- include('../includes/navbar.ejs')  %> 

    <!-- main -->
    <main>
      <div class="links">
        <img src="../img/login/rechtsImg.png" />
      </div>
      <div class="rechts">
        <div class="cirlecs">
          <div class="circel-1"></div>
          <div class="circel-2"></div>
          <div class="circel-3"></div>
          <div class="circel-4"></div>
          <div class="circel-5"></div>
        </div>
        <div class="form-container">
          <div class="top-box">
            <h1>Welkom terug !</h1>
            <p>Log in om door te gaan</p>
          </div>
          <form action="/login" method="POST">
            <div class="input-box">
              <label for="email"><i class="fas fa-envelope"></i></label>
              <input type="email" name="email" placeholder="Vul uw email in" />
            </div>

            <div class="input-box">
              <label for="password"><i class="fas fa-lock"></i></label>
              <input type="password" name="password" placeholder="Vul uw wachtwoord in" />
            </div>
            <a href="#"><p>Wachtwoord vergeten?</p></a>
            <button type="submit">Log in</button>
            <a href="/signup"
              ><p>Heb je geen account?<span>Sing up</span></p></a
            >
          </form>
        </div>
      </div>
    </main>
